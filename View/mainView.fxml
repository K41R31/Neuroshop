<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.shape.Rectangle?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Neuroshop.Controller.MainViewController">
   <children>
      <AnchorPane fx:id="introPane_AP" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
      <VBox fx:id="mainPane_AP" alignment="TOP_CENTER" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <HBox alignment="CENTER_LEFT" maxHeight="-Infinity" minHeight="-Infinity" prefHeight="30.0" styleClass="borderPane" stylesheets="@Css/mainViewCss.css">
               <children>
                  <Text fill="#979797" strokeType="OUTSIDE" strokeWidth="0.0" styleClass="borderText" text="NEUROSHOP" textAlignment="CENTER">
                     <font>
                        <Font name="LEIXO DEMO" size="22.0" />
                     </font>
                  </Text>
                  <Region fx:id="frameBorder_RE" HBox.hgrow="ALWAYS" />
                  <HBox>
                     <children>
                        <ImageView fitHeight="30.0" fitWidth="45.0" onMousePressed="#minimize" pickOnBounds="true" preserveRatio="true" styleClass="minimizeButton" />
                        <ImageView fitHeight="30.0" fitWidth="45.0" onMousePressed="#minMax" pickOnBounds="true" preserveRatio="true" styleClass="minmaxButton" />
                        <ImageView fitHeight="30.0" fitWidth="45.0" onMousePressed="#exit" pickOnBounds="true" preserveRatio="true" styleClass="closeButton" />
                     </children>
                  </HBox>
               </children>
               <VBox.margin>
                  <Insets />
               </VBox.margin>
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
            </HBox>
            <StackPane fx:id="whiteboardPane_SP" maxHeight="-Infinity" minHeight="-Infinity" styleClass="mainPane" stylesheets="@Css/mainViewCss.css">
               <children>
                  <VBox prefHeight="200.0" prefWidth="100.0" />
                  <Rectangle fx:id="testmoveObject" arcHeight="5.0" arcWidth="5.0" fill="DODGERBLUE" height="200.0" stroke="BLACK" strokeType="INSIDE" width="200.0" />
                  <StackPane fx:id="testmoveObject2" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="100.0" prefWidth="100.0" style="-fx-background-color: BLACK;" />
               </children>
            </StackPane>
         </children>
      </VBox>
   </children>
</AnchorPane>
