<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.shape.*?>
<?import javafx.scene.text.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.Group?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.shape.Circle?>
<?import javafx.scene.shape.Rectangle?>
<?import javafx.scene.text.Text?>

<StackPane fx:id="rootPane" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Neuroshop.Widgets.NeuralNetWidget.NeuralNetController">
   <children>
      <VBox alignment="CENTER">
         <children>
            <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true" rotate="-90.0">
               <image>
                  <Image url="@../../Ressources/Assets/toolsMenuButton.png" />
               </image>
               <VBox.margin>
                  <Insets />
               </VBox.margin>
            </ImageView>
            <VBox alignment="CENTER" maxHeight="-Infinity" minHeight="-Infinity" prefHeight="60.0">
               <children>
                  <HBox maxHeight="-Infinity">
                     <children>
                        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Text">
                           <HBox.margin>
                              <Insets left="10.0" right="5.0" />
                           </HBox.margin>
                        </Text>
                        <Text layoutX="10.0" layoutY="23.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Text">
                           <HBox.margin>
                              <Insets left="5.0" right="5.0" />
                           </HBox.margin>
                        </Text>
                        <Text layoutX="33.0" layoutY="23.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Text">
                           <HBox.margin>
                              <Insets left="5.0" right="5.0" />
                           </HBox.margin>
                        </Text>
                     </children>
                  </HBox>
                  <HBox>
                     <children>
                        <Rectangle arcHeight="5.0" arcWidth="5.0" fill="DODGERBLUE" height="20.0" stroke="BLACK" strokeType="INSIDE" width="20.0">
                           <HBox.margin>
                              <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                           </HBox.margin>
                        </Rectangle>
                        <Rectangle arcHeight="5.0" arcWidth="5.0" fill="DODGERBLUE" height="20.0" layoutX="20.0" layoutY="20.0" stroke="BLACK" strokeType="INSIDE" width="20.0">
                           <HBox.margin>
                              <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                           </HBox.margin>
                        </Rectangle>
                        <Region HBox.hgrow="ALWAYS" />
                        <Rectangle arcHeight="5.0" arcWidth="5.0" fill="DODGERBLUE" height="20.0" stroke="BLACK" strokeType="INSIDE" width="20.0">
                           <HBox.margin>
                              <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                           </HBox.margin>
                        </Rectangle>
                     </children>
                  </HBox>
               </children>
            </VBox>
            <Group>
               <children>
                  <AnchorPane fx:id="splinesPane" />
                  <HBox fx:id="vBoxContainer" alignment="CENTER">
                     <children>
                        <VBox fx:id="inputColumns" alignment="CENTER" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="200.0" prefWidth="130.0" spacing="30.0">
                           <children>
                              <Circle fill="WHITE" radius="20.0" stroke="BLACK" strokeType="INSIDE" />
                              <Circle radius="20.0" stroke="BLACK" strokeType="INSIDE" strokeWidth="0.0" />
                              <Circle layoutX="75.0" layoutY="145.0" radius="20.0" stroke="BLACK" strokeType="INSIDE" strokeWidth="0.0" />
                              <Circle fill="WHITE" layoutX="75.0" layoutY="40.0" radius="20.0" stroke="BLACK" strokeType="INSIDE" />
                           </children>
                           <padding>
                              <Insets bottom="30.0" top="30.0" />
                           </padding>
                        </VBox>
                        <VBox fx:id="hiddenLayer" alignment="CENTER" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="200.0" prefWidth="130.0" spacing="30.0">
                           <children>
                              <Circle radius="20.0" stroke="BLACK" strokeType="INSIDE" strokeWidth="0.0" />
                              <Circle layoutX="75.0" layoutY="110.0" radius="20.0" stroke="BLACK" strokeType="INSIDE" strokeWidth="0.0" />
                              <Circle layoutX="75.0" layoutY="75.0" radius="20.0" stroke="BLACK" strokeType="INSIDE" strokeWidth="0.0" />
                              <Circle layoutX="75.0" layoutY="110.0" radius="20.0" stroke="BLACK" strokeType="INSIDE" strokeWidth="0.0" />
                              <Circle layoutX="75.0" layoutY="215.0" radius="20.0" stroke="BLACK" strokeType="INSIDE" strokeWidth="0.0" />
                              <Circle layoutX="75.0" layoutY="250.0" radius="20.0" stroke="BLACK" strokeType="INSIDE" strokeWidth="0.0" />
                           </children>
                           <padding>
                              <Insets bottom="30.0" top="30.0" />
                           </padding>
                        </VBox>
                        <VBox fx:id="outputLayer" alignment="CENTER" layoutX="140.0" layoutY="10.0" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="200.0" prefWidth="130.0" spacing="30.0">
                           <children>
                              <Circle radius="20.0" stroke="BLACK" strokeType="INSIDE" strokeWidth="0.0" />
                           </children>
                           <padding>
                              <Insets bottom="30.0" top="30.0" />
                           </padding>
                        </VBox>
                     </children>
                  </HBox>
               </children>
            </Group>
            <VBox alignment="CENTER">
               <children>
                  <Slider fx:id="sliderWidth" max="10.0" onMouseDragged="#changeWidth" value="1.0" />
                  <Slider fx:id="sliderTangent" max="200.0" onMouseDragged="#changeTangent" value="50.0" />
                  <Button mnemonicParsing="false" onAction="#drawSplines" text="Button" />
               </children></VBox>
         </children>
      </VBox>
   </children>
</StackPane>
