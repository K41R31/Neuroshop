<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Group?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<StackPane fx:id="rootPane" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Neuroshop.Gui.Widgets.NeuralNetWidget.NeuralNetController">
   <children>
      <VBox alignment="CENTER">
         <children>
            <HBox fx:id="menuPane" maxHeight="-Infinity" minHeight="-Infinity" prefHeight="100.0">
               <children>
                  <VBox maxWidth="-Infinity" minWidth="-Infinity" prefWidth="130.0" />
               </children>
            </HBox>
            <Group>
               <children>
                  <AnchorPane fx:id="splinesPane" />
                  <HBox fx:id="vBoxContainer" alignment="CENTER">
                     <children>
                        <VBox fx:id="inputLayer" alignment="CENTER" maxWidth="-Infinity" minWidth="-Infinity" prefWidth="130.0" spacing="30.0">
                           <padding>
                              <Insets bottom="30.0" top="30.0" />
                           </padding>
                        </VBox>
                        <HBox fx:id="hiddenLayer" />
                        <VBox fx:id="outputLayer" alignment="CENTER" layoutX="140.0" layoutY="10.0" maxWidth="-Infinity" minWidth="-Infinity" prefWidth="130.0" spacing="30.0">
                           <padding>
                              <Insets bottom="30.0" top="30.0" />
                           </padding>
                        </VBox>
                     </children>
                  </HBox>
               </children>
            </Group>
         </children>
      </VBox>
   </children>
</StackPane>
