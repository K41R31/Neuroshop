<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Group?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.shape.Circle?>

<StackPane fx:id="rootPane" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Neuroshop.Gui.Widgets.NeuralNetWidget.NeuralNetController">
   <children>
      <VBox alignment="CENTER_RIGHT" style="-fx-border-color: #181818;">
         <children>
            <Group>
               <children>
                  <AnchorPane fx:id="splinesPane" />
                  <HBox fx:id="vBoxContainer" alignment="CENTER">
                     <children>
                        <VBox fx:id="inputLayer" alignment="CENTER" maxWidth="-Infinity" minWidth="-Infinity" prefWidth="130.0" spacing="30.0">
                           <padding>
                              <Insets bottom="30.0" top="30.0" />
                           </padding>
                           <children>
                              <Circle fill="WHITE" radius="20.0" stroke="BLACK" strokeType="INSIDE" />
                              <Circle fill="WHITE" layoutX="75.0" layoutY="60.0" radius="20.0" stroke="BLACK" strokeType="INSIDE" />
                              <Circle fill="WHITE" layoutX="75.0" layoutY="130.0" radius="20.0" stroke="BLACK" strokeType="INSIDE" />
                              <Circle fill="WHITE" layoutX="75.0" layoutY="200.0" radius="20.0" stroke="BLACK" strokeType="INSIDE" />
                           </children>
                        </VBox>
                        <HBox fx:id="hiddenLayer" />
                        <VBox fx:id="outputLayer" alignment="CENTER" layoutX="140.0" layoutY="10.0" maxWidth="-Infinity" minWidth="-Infinity" prefWidth="130.0" spacing="30.0">
                           <padding>
                              <Insets bottom="30.0" top="30.0" />
                           </padding>
                           <children>
                              <Circle fill="WHITE" radius="20.0" stroke="BLACK" strokeType="INSIDE" />
                           </children>
                        </VBox>
                     </children>
                  </HBox>
               </children>
            </Group>
            <StackPane maxWidth="-Infinity" minWidth="-Infinity" prefWidth="170.0">
               <children>
                  <ImageView fitHeight="60.0" fitWidth="170.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../../../Ressources/Assets/hiddenLayerPanel.png" />
                     </image>
                  </ImageView>
                  <HBox alignment="CENTER" spacing="10.0">
                     <children>
                        <ImageView fitHeight="30.0" fitWidth="30.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../../../Ressources/Assets/crossButton.png" />
                           </image>
                        </ImageView>
                        <ImageView fitHeight="30.0" fitWidth="30.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../../../Ressources/Assets/deleteHdnLayer.png" />
                           </image>
                        </ImageView>
                     </children>
                     <padding>
                        <Insets left="50.0" />
                     </padding>
                  </HBox>
               </children>
            </StackPane>
            <HBox prefHeight="100.0" prefWidth="200.0" style="-fx-background-color: #2f2f2f;">
               <children>
                  <Region maxWidth="-Infinity" minWidth="-Infinity" prefWidth="130.0" />
                  <VBox maxWidth="-Infinity" minWidth="-Infinity" prefWidth="130.0" />
               </children>
               <VBox.margin>
                  <Insets top="-5.0" />
               </VBox.margin>
            </HBox>
            <StackPane maxHeight="-Infinity" minHeight="-Infinity" prefHeight="35.0">
               <children>
                  <ImageView fitHeight="35.0" fitWidth="112.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../../../Ressources/Assets/nnOpener.png" />
                     </image>
                  </ImageView>
               </children>
            </StackPane>
         </children>
      </VBox>
   </children>
</StackPane>
