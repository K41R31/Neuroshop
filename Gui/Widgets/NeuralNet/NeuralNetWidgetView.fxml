      <AnchorPane fx:id="splinesPane">
         <children>
            <HBox fx:id="vBoxContainer" alignment="CENTER" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
               <children>
                  <VBox alignment="CENTER" maxWidth="-Infinity" minWidth="-Infinity" prefWidth="130.0" spacing="40.0">
                     <children>
                        <Circle radius="20.0" stroke="BLACK" strokeType="INSIDE" strokeWidth="0.0" />
                        <Circle radius="20.0" stroke="BLACK" strokeType="INSIDE" strokeWidth="0.0" />
                     </children>
                     <padding>
                        <Insets bottom="20.0" top="20.0" />
                     </padding>
                  </VBox>
                  <VBox alignment="CENTER" maxWidth="-Infinity" minWidth="-Infinity" prefWidth="130.0" spacing="40.0">
                     <children>
                        <Circle layoutX="70.0" layoutY="150.0" radius="20.0" stroke="BLACK" strokeType="INSIDE" strokeWidth="0.0" />
                     </children>
                     <padding>
                        <Insets bottom="20.0" top="20.0" />
                     </padding>
                  </VBox>
               </children>
            </HBox>
         </children>
      </AnchorPane>
      <StackPane fx:id="menuPane" maxHeight="-Infinity" minHeight="-Infinity" prefHeight="80.0">
         <children>
            <Rectangle arcHeight="20.0" arcWidth="20.0" fill="TRANSPARENT" height="50.0" stroke="BLACK" strokeType="INSIDE" width="50.0" StackPane.alignment="CENTER" />
            <Line endX="15.0" endY="-15.0" startX="-15.0" startY="15.0" />
            <Line endX="15.0" endY="15.0" startX="-15.0" startY="-15.0" />
         </children>
      </StackPane>
   </children>
</VBox>
